{
  "name": "express-starter",
  "version": "1.0.0",
  "description": "components from npx rapidrestify create-node-app",
  "main": "app.js",
  "scripts": {
    "start": "node --trace-warnings app.js",
    "server": "nodemon",
    
    "db-push": "prisma db push",
    "db-pull": "prisma db pull",
    "db-start": "prisma studio",
    "db-seed": "prisma db seed",

    "db-push-mongo": "prisma db push --schema=./prisma/mongo-schema.prisma",
    "db-pull-mongo": "prisma db pull --schema=./prisma/mongo-schema.prisma",
    "db-start-mongo": "prisma studio --schema=./prisma/mongo-schema.prisma",
    "db-seed-mongo": "prisma db seed --schema=./prisma/mongo-schema.prisma",
    "db-generate-mongo": "prisma db generate --schema=./prisma/mongo-schema.prisma",

    "db-push-mysql": "prisma db push --schema=./prisma/mysql-schema.prisma",
    "db-pull-mysql": "prisma db pull --schema=./prisma/mysql-schema.prisma",
    "db-start-mysql": "prisma studio --schema=./prisma/mysql-schema.prisma",
    "db-seed-mysql": "prisma db seed --schema=./prisma/mysql-schema.prisma",
    "db-generate-mysql": "prisma db generate --schema=./prisma/mysql-schema.prisma",
    
    "css": "npx tailwindcss -i ./views/index.css -o ./public/style.css --watch",
    "dev": "concurrently --restart-tries 3  \"npm run server\" \"npm run css\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.1.0",
    "cors": "^2.8.5",
    "csrf-csrf": "^3.0.4",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.2.0",
    "express-session": "^1.18.0",
    "express-subdomain": "^1.0.6",
    "fs-jetpack": "^5.1.0",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "knex-paginate": "^3.1.2",
    "mongoose": "^8.3.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.7",
    "nodemailer": "^6.9.13",
    "perfect-express-sanitizer": "^2.0.2",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1",
    "session-file-store": "^1.5.0",
    "socket.io": "^4.7.5",
    "validatorjs": "^3.22.1",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "prisma": "^5.12.1",
    "tailwindcss": "^3.4.3"
  }
}
